{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "course-goals-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/courseGoals/CourseGoalsWebPart.js",
          "manifest": "./src/webparts/courseGoals/CourseGoalsWebPart.manifest.json"
        }
      ]
    },
    "duplicate-page-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/duplicatePage/DuplicatePageWebPart.js",
          "manifest": "./src/webparts/duplicatePage/DuplicatePageWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CourseGoalsWebPartStrings": "lib/webparts/courseGoals/loc/{locale}.js",
    "DuplicatePageWebPartStrings": "lib/webparts/duplicatePage/loc/{locale}.js", 
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}
